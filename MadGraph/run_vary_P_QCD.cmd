# 1j, QCD
import DMsimp_s_spin0
generate p p > xd xd~ l+ l- [QCD]
add process p p > xd xd~ l+ l- j [QCD]
output DMsimp_s_spin0P_0j_10_1_QCD
launch
shower=ON
done
./Cards/param_card_DMs0P_10_1.dat
./Cards/shower_card_DMs1_0j.dat
./Cards/run_card_DMs1.dat
done

